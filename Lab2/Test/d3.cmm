struct Node{
	int num;
	int value;
	int next;
};

struct Node2{
	int num2;
	int value2;
	int next2;
};

int link(struct Node n1, struct Node n2)
{
	if(n1.value >= n2.value){
		n1.next = n2.num;
		return 0;
	}
	else{
		n2.next = n1.num;
		return 1;
	}
}

int main()
{
	int i = 0;
	int s = 0;
	struct Node a[10];
	struct Node2 b[10];
	while(i < 10)
	{
		a[i].num = i;
		a[i].value = i*i-5*i;
		a[i].next = i + 1;
		b[i].num2 = i;
		b[i].value2 = i*i-5*i;
		b[i].next2 = i+1;
		i = i+1;
	}
	i=0;

	while(i<10)
	{
		while(s<10)
		{
			if(link(a[i],b[s])==1)
			{
				a[i].value = a[s].value;
				a[i].next = i + 1;
			}
			s = s + 1;
		}

		s = 0;
		i = i+1;
	}
	return 0;
}
